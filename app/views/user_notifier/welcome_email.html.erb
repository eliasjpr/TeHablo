<table width="600" align="center" style=" border-radius:10px; border: 1px solid #e9e9e9; border-radius:10px">
  <tbody>
  <tr>
    <td style=" padding:20px; background-color: white;">
      <%= image_tag @user.agent.logo.url(:thumb), class: '' %>
    </td>
    <td style="font-size: 16px; color: #a3a4a8; text-align: left; font-weight: light; font-family: Helvetica, Arial, sans-serif; background-color:white; ">
      <strong>Welcome </strong> notification
    </td>
  </tr>
  <tr>
    <td colspan="2" style=" padding:0.25em; background-color: #e9e9e9; line-height: 16px;">
    </td>
  </tr>

  <tr>
    <td style=" padding:20px; background-color: white; font-size: 16px; color: #b8b9c1; font-weight: normal; font-family: Helvetica, Arial, sans-serif; line-height: 26px; vertical-align: top;">
      <%= @user.full_name %> <br/>
      <%= "#{@user.id}-#{@user.created_at.strftime('%Y%m%d%H%m%s')}" %>
      <br/>
      <%= @user.created_at.strftime('%m/%d/%y at %H:%m') %>
    </td>
    <td style=" padding:20px; background-color: white; font-size: 16px; color: #b8b9c1; font-weight: normal; font-family: Helvetica, Arial, sans-serif; line-height: 26px; vertical-align: top;"></td>
  </tr>
  <tr>
    <td colspan="2" style=" padding:20px; background-color: white; font-size: 16px; color: #b8b9c1; font-weight: normal;font-family: Helvetica, Arial, sans-serif; line-height: 26px; vertical-align: top;">

      <%= t('email.welcome_html', login_url: link_to(login_path, login_path), email: @user.email) %>

      <hr>
      <table>
        <tr>
          <td>
            <strong><%= t('services.titles.tehablo') %></strong>

            <p><%= t('services.text.tehablo_html') %></p>
          </td>
          <td>
            <strong><%= t('services.titles.followme') %></strong>

            <p><%= t('services.text.followme_html') %></p>
          </td>
          <td>
            <%= t('services.text.to_make_call') %>
            <strong><%= t('services.titles.access_numbers') %></strong>
            <%= t('services.text.access_numbers_html') %>

          </td>
        </tr>
      </table>
    </td>
  </tr>
  </tbody>
</table>